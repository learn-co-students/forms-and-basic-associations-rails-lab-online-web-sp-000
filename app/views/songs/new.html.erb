<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <%= f.text_field :song_genre_id, list: "song_genre_id" %>
  <datalist id="song_genre_id">
    <% Genre.all.each do |genre| %>
      <option value="<%= genre.name %>">
    <% end %>
  </datalist>
  <%= f.submit %>
<% end %>


