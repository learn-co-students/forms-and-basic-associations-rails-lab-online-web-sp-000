<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :artist %>
  <%= f.text_field :artist_name %>
  <%= f.label :genre_id %>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %><br /><br>
  <label for="song_notes">Note</label>
  <input name="song[note_contents][]" id='song_notes_1'><br>
  <label for="song_notes">Note</label>
  <input name="song[note_contents][]" id='song_notes_2'><br>
  <label for="song_notes">Note</label>
  <input name="song[note_contents][]" id='song_notes_3'><br><br>

  <%= f.submit %>
<% end %>


