<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.text_field :title %>
  <%= f.text_field :artist_name%>
  <%= f.text_field :genre_id, list: "genres_autocomplete" %>
  <datalist id="genres_autocomplete">
    <% Genre.all.each do |genre| %>
      <option value="<%= genre.id %>"><%= genre.name %></option>
    <% end %>
  </datalist>
  <%= f.text_field :notes_1%>
  <%= f.text_field :notes_2%>
  <br />

  <%= f.submit %>
<% end %>


