<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <br>

  <%= f.label :artist %>
  <%= f.text_field :artist_name %>
  <br>
  
  Genre: <%= f.collection_select :genre_name, Genre.all, :name, :name %>
  <br>
  Notes: <br>
  <input type="text" name="song[note_contents][]" id="song_notes_1"><br>
  <input type="text" name="song[note_contents][]" id="song_notes_2"><br>
  <input type="text" name="song[note_contents][]" id="song_notes_3"><br>

  
  <%= f.submit %>
<% end %>


