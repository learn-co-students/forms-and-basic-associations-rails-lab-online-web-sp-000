<h1>Create a Song</h1>
<% byebug %>
<%= form_for @song  do |f| %>
  <div>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>

  <div>
    <%= f.label :artist_name %>
    <%= f.text_field :artist_name %>
  </div>

  <div>
    <%= f.label :genre_name %>
    <%= f.collection_select :genre_id, Genre.all, :id, :name %>
  </div>

  <div>
  <ul>
    <li><input name='song[note_contents][]' id='song_notes_1'></li>
    <li><input name='song[note_contents][]' id='song_notes_2'></li>
    <li><input name='song[note_contents][]' id='song_notes_3'></li>
  </ul>
  </div>

  <%= f.submit %>

<% end %>


