<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %><br>
  <%= f.text_field :title %><br>
  <%= f.label :song_artist_name %><br>
  <%= f.text_field :artist_name %><br>
  <%= f.label :song_genre_name %><br>
  <%=f.collection_select :genre_id, Genre.all, :id, :name %><br>
  <input name="song[note_contents][]" id='song_notes_attributes_0_content'><br>
  <input name="song[note_contents][]" id='song_notes_attributes_1_content'><br>
  <input name="song[note_contents][]" id='song_notes_attributes_2_content'><br>
  <%= f.submit %>
<% end %>


