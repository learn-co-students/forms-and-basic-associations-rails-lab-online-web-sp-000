<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <%= f.label :genre_name %>
  <%= f.collection_select :genre_name, Genre.all, :name, :name %>
  <%= f.label :note_content %>
  <%= f.text_field :note_content %>
  <%= f.label :note_content %>
  <%= f.text_field :note_content %>
  <%= f.label :note_content %>
  <%= f.text_field :note_content %>
  <br/>
  <%= f.submit %>
<% end %>



