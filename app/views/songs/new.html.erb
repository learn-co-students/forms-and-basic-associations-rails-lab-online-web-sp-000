<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <%= f.label :genre_name %>
  <%= f.collection_select(:genre_id, Genre.all, :id, :name) %>
  <%= f.label :notes %>
  <input id="song_notes_1" name="song[notes][]" type="text">
  <input id="song_notes_2" name="song[notes][]" type="text">
  <br />

  <%= f.submit %>
<% end %>


