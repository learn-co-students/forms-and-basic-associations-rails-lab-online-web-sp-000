<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br>
  <%= f.label :song_artist_name %>
  <%= f.text_field :artist_name %><br>

   <%= f.collection_select :genre_id, Genre.all, :id, :name %> <br>
  Note 1:<textarea name="song_notes_1"></textarea><br>
  Note: 2 <textarea name="song_notes_2"></textarea><br>

  <%= f.submit %>
<% end %>


