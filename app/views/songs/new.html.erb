<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %>
  <%= f.text_field :title %>
  <input name="song[note_ids][]" id="song_notes_1">
  <input name="song[note_ids][]" id="song_notes_2">
  <br />

  <%= f.submit %>
<% end %>


