<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %><br>
  <%= f.text_field :title %><br>
  <%= f.label :artist_name %><br>
  <%= f.text_field :artist_name %><br>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %><br>
  <textarea  name="song[note_contents][]" id="song_notes_1"></textarea><br>
  <textarea  name="song[note_contents][]" id="song_notes_2"></textarea><br> 
  
  
  <br />

  <%= f.submit %>
<% end %>


