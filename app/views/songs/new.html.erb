  
<%= form_for @song do |f| %>
  <%= f.text_field :title %>
  <%= f.text_field :artist_name %>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %>
  <%= f.fields_for :notes do |note|
    <%= note.label :note_contents %>
    <%= note.text_field :note_contents %>
    <%end%>


  <% end %>

