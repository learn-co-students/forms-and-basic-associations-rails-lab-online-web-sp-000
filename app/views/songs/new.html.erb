<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title , name: "song_title"%>
  <%= f.text_field :title %>
  <br />
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name, id: "song_artist_name" %>

  <%= f.label :genre_name %>
  <%= f.collection_select :genre_id, Genre.all, :id, :name %>
  <br />
  <input name='song[note_contents][]' id ='song_notes_attributes_0_content'>
  <input name='song[note_contents][]' id='song_notes_attributes_1_content'>
  <input name='song[note_contents][]' id='song_notes_attributes_2_content'>
  <%= f.submit %>
<% end %>
