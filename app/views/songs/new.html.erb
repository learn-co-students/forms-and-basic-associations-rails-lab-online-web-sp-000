<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.text_field :artist_name %>
  <input name="song[notes][]" id="song_notes_attributes_0_content" >
  <input name="song[notes][]" id="song_notes_attributes_1_content" >
  <%= f.collection_select :genre_id, Genre.all, :name, :name %>
  <br />

  <%= f.submit %>
<% end %>
