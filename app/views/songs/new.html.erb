<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :song_artist_name %>
  <%= f.text_field :artist_name %>

  <%= f.collection_select :genre_id, Genre.all, :id, :name%>
  <br />
  
 <input name="song[song_notes][]" id="song_notes_1">
<input name="song[song_notes][]" id="song_notes_2">
<input name="song[song_notes][]" id="song_notes_3">
<input type="submit" value="Submit">
  <%= f.submit %>
<% end %>


