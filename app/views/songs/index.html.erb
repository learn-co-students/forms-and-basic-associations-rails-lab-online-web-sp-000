<h1>All songs</h1>

<% @songs.each do |song| %>
  <span class=song-title><%= song.title %></span>
  <span class=song-artist><%= song.artist.name %></span>
  <span class=song-genre><%= song.genre.name %></span>
  <ul class=song-notes>
    <% song.notes.each do |note| %>
      <li class=song-note><%= note.content %></li>
    <% end %>
  </ul>

<% end %>
